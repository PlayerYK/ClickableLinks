{
  "manifest_version": 3,
  "name": "Clickable Links",
  "version": "1.0.0",
  "description": "Turns unclickable URLs and email addresses into clickable ones.",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {}
}